{
  "name": "nextjs-starter",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "next build",
    "build-storybook": "build-storybook --docs",
    "check-todos": "leasot src/**/*.{js,jsx,ts,tsx}",
    "commit": "git-cz",
    "dev": "next dev",
    "lint": "npx prettier --write .",
    "sort": "sort-package-json",
    "postinstall": "husky install && rm -rf .git/hooks && ln -s ../.husky .git/hooks",
    "start": "next start",
    "storybook": "start-storybook -p 6006"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "**/*.{md,yml,json,babelrc,eslintrc,prettierrc}": [
      "npx prettier --write"
    ],
    "**/*.{ts,tsx,js,jsx}": [
      "npx prettier --write"
    ],
    "package.json": [
      "npx sort-package-json"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@dracula/dracula-ui": "^0.7.0",
    "next": "10.1.3",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-icons": "^4.2.0",
    "react-spring": "^9.1.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.1",
    "@commitlint/config-conventional": "^12.1.1",
    "@storybook/addon-actions": "^6.2.8",
    "@storybook/addon-essentials": "^6.2.8",
    "@storybook/addon-links": "^6.2.8",
    "@storybook/addons": "^6.2.8",
    "@storybook/react": "^6.2.8",
    "@storybook/theming": "^6.2.8",
    "@types/node": "^14.14.37",
    "@types/react": "^17.0.3",
    "autoprefixer": "^10.2.5",
    "commitizen": "^4.2.3",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^6.0.0",
    "leasot": "^12.0.0",
    "lint-staged": "^10.5.4",
    "postcss": "^8.2.10",
    "prettier": "^2.2.1",
    "sort-package-json": "^1.49.0",
    "tailwindcss": "^2.1.1",
    "typescript": "^4.2.4"
  },
  "czConfig": {
    "path": "node_modules/cz-conventional-changelog"
  }
}
